{

    "express": {
        "view cache": false,
        "view engine": "js",
        "views": "path:./.build/templates"
    },

    "view engines": {
        "js": {
            "module": "engine-munger",
            "renderer": {
                "method": "js",
                "arguments": [
                    { "cache": true },
                    {
                        "views": "config:express.views",
                        "view engine": "config:express.view engine",
                        "specialization": "config:specialization",
                        "i18n": "config:i18n"
                    }
                ]
            }
        }
    },
    "redisconfig": {
    	"isRedis":0,
        "host": "127.0.0.1",
        "port": "6379"
    },
    "databaseConfig2": {
    "user": "root",
    "password": "goumao",
        "host": "localhost",
        "database": "yuechang"
    },
"databaseConfig1": {
    "user": "root",
    "password": "yuShu1234",
        "host": "121.199.55.129",
        "database": "yuechang"
    },
    "databaseConfig": {
    "user": "root",
    "password": "yuShu1234",
        "host": "121.199.55.129",
        "database": "shengzhu"
    },
    "i18n": {
        "contentPath": "path:./locales",
        "fallback": "zh-CN"
    },
    
    

    "middleware": {

        "static": {
            "module": {
                "arguments": [ "path:./.build" ]
            }
        },

        "router": {
            "module": {
                "arguments": [{ "directory": "path:./controllers" }]
            }
        },

		"auth": {
            "priority": 90,
            "enabled": true,
            "module": {
                "name": "path:./lib/auth"
            }
        },
 		"redis": {
            "priority": 91,
            "enabled": false,
            "module": {
                "name": "path:./lib/redis"
            }
        },
        "locale": {
            "priority": 95,
            "enabled": true,
            "module": {
                "name": "path:./lib/locale"
            }
        },
        
        "expressView": {
            "priority": 100,
            "enabled": true,
            "module": {
                "name": "engine-munger/lib/expressView"
            }
        },
        "redisAfter": {
            "priority": 101,
            "enabled": false,
            "module": {
                "name": "path:./lib/redisAfter"
            }
        },

        "fileNotFound": {
            "enabled": true,
            "priority": 130,
            "module": {
                "name": "kraken-js/middleware/404",
                "arguments": [ "errors/404" ]
            }
        },

        "serverError": {
            "enabled": true,
            "priority": 140,
            "module": {
                "name": "kraken-js/middleware/500",
                "arguments": [ "errors/500" ]
            }
        }

    }
}
